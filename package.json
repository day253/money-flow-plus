{
  "name": "money-flow-plus",
  "displayName": "💸 Money Flow Plus | 收入时钟",
  "description": "让你在写代码时感受金钱流入的快感！实时展示收入增长，激励你持续创造价值。| Watch your money flow while you code!",
  "version": "0.0.1",
  "publisher": "day253",
  "author": {
    "name": "day253"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/day253/money-flow-plus"
  },
  "bugs": {
    "url": "https://github.com/day253/money-flow-plus/issues"
  },
  "homepage": "https://github.com/day253/money-flow-plus#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "money",
    "income",
    "earnings",
    "motivation",
    "productivity"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFD700",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Daily Earnings",
      "properties": {
        "dailyEarnings.monthlyIncome": {
          "type": "number",
          "default": 1000000,
          "description": "你的月收入"
        },
        "dailyEarnings.workingDays": {
          "type": "number",
          "default": 22,
          "description": "每月工作天数"
        },
        "dailyEarnings.workingHours": {
          "type": "number",
          "default": 8,
          "description": "每天工作小时数"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "compile": "echo Compilation not needed"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "eslint": "^8.54.0",
    "sharp": "^0.33.5"
  },
  "license": "MIT"
}
