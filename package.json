{
  "name": "money-flow-plus",
  "displayName": "Money Flow Plus | Income Timer",
  "description": "Track your income in real-time while coding! Motivate yourself by watching your earnings grow. | 让写代码充满动力，实时展示收入增长！",
  "version": "0.0.1",
  "publisher": "day253",
  "author": {
    "name": "day253"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/day253/money-flow-plus"
  },
  "bugs": {
    "url": "https://github.com/day253/money-flow-plus/issues"
  },
  "homepage": "https://github.com/day253/money-flow-plus#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "income",
    "earnings",
    "motivation",
    "productivity",
    "timer"
  ],
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFD700",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "configuration": {
      "title": "Money Flow Plus",
      "properties": {
        "dailyEarnings.monthlyIncome": {
          "type": "number",
          "default": 10000,
          "description": "Your monthly income | 你的月收入"
        },
        "dailyEarnings.workingDays": {
          "type": "number",
          "default": 22,
          "description": "Working days per month | 每月工作天数"
        },
        "dailyEarnings.workingHours": {
          "type": "number",
          "default": 8,
          "description": "Working hours per day | 每天工作小时数"
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./",
    "compile": "echo Compilation not needed"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "eslint": "^8.54.0",
    "sharp": "^0.33.5"
  },
  "license": "MIT"
}
